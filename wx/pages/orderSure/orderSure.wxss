/*支付二维码*/
.pay-box{
  position: absolute;
  width: 82%;
  left: 9%;
  top: 9%;
  background: #fff;
  border-radius: 13rpx;
  padding: 45rpx 40rpx 76rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.code-title{
  line-height: 56rpx;
  font-size: 40rpx;
  color: #111;
}
.code-disc{
  font-size: 27rpx;
  line-height: 47rpx;
  color: #555;
  padding: 35rpx 0 42rpx;
  word-break: break-all;
  border-bottom: 1px dashed #999;
  position: relative;
}
.code-disc::before{
  display: block;
  content: '';
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  background: rgba(0,0,0,.6);
  bottom: -18rpx;
  left: -58rpx;
  border-radius: 18rpx;
}
.code-disc::after{
  display: block;
  content: '';
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  background: rgba(0,0,0,.6);
  bottom: -18rpx;
  right: -58rpx;
  border-radius: 18rpx;
}

.code-disc1{
  font-weight: bold;
}
.code-disc2{
  color: #F5443F;
}


.code-pic{
  padding: 80rpx 0 35rpx;
  width: 278rpx;
  height: 278rpx;
  margin: 0 auto;
  position: relative;
}
.code-pic image{
  width: 100%;
